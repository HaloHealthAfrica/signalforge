# TradeLoop Frontend Production Configuration
VITE_API_URL=https://tradeloop-api.onrender.com
VITE_APP_TITLE=TradeLoop
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_BACKTEST=true
VITE_ENABLE_PORTFOLIO_SYNC=true

# Optimized Polling Intervals for Production
VITE_MARKET_DATA_INTERVAL=5000
VITE_SIGNALS_INTERVAL=10000
VITE_PORTFOLIO_INTERVAL=15000

# Error Reporting
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_ANALYTICS=true

# Demo/Testing API Keys (not real)
VITE_DEMO_POLYGON_KEY=demo_polygon_api_key_12345
VITE_DEMO_TWELVEDATA_KEY=demo_twelvedata_api_key_67890
VITE_DEMO_TRADIER_KEY=demo_tradier_api_key_abcde