# TradeLoop Frontend Docker Configuration
VITE_API_URL=http://tradeloop:3000
VITE_APP_TITLE=TradeLoop
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_BACKTEST=true
VITE_ENABLE_PORTFOLIO_SYNC=true

# Docker Optimized Intervals
VITE_MARKET_DATA_INTERVAL=5000
VITE_SIGNALS_INTERVAL=10000
VITE_PORTFOLIO_INTERVAL=15000

# Disable development features
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_ANALYTICS=false